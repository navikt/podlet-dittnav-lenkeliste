{
  "name": "podlet-dittnav-lenkeliste",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "6.0.0",
    "@metrics/prometheus-consumer": "3.0.1",
    "@podium/podlet": "4.4.14",
    "@sentry/browser": "5.29.2",
    "classnames": "2.2.6",
    "craco-less": "1.17.1",
    "express": "4.17.1",
    "less": "3.13.1",
    "nav-frontend-core": "5.0.10",
    "nav-frontend-lenker": "1.0.55",
    "nav-frontend-lenker-style": "0.2.41",
    "nav-frontend-typografi": "2.0.39",
    "nav-frontend-typografi-style": "1.0.31",
    "prom-client": "13.0.0",
    "prop-types": "15.7.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "swr": "0.4.0"
  },
  "devDependencies": {
    "@eik/rollup-plugin": "4.0.2",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "17.1.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "11.2.1",
    "@rollup/plugin-replace": "2.4.2",
    "@testing-library/jest-dom": "5.11.6",
    "@testing-library/react": "11.2.2",
    "@testing-library/user-event": "12.6.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "husky": "4.3.6",
    "lint-staged": "10.5.3",
    "msw": "0.24.2",
    "postcss": "8.2.8",
    "prettier": "2.2.1",
    "react-scripts": "4.0.1",
    "rollup": "2.43.1",
    "rollup-plugin-livereload": "2.0.0",
    "rollup-plugin-postcss": "4.0.0",
    "rollup-plugin-postcss-webpack-alias-less-loader": "1.0.0",
    "rollup-plugin-serve": "1.1.0",
    "rollup-plugin-terser": "7.0.2"
  },
  "scripts": {
    "start": "rollup -c rollup.config.dev.js -w",
    "build": "rollup -c rollup.config.prod.js",
    "test": "npm run lint && react-scripts test",
    "test-ci": "npm run lint && CI=true react-scripts test",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm i",
    "lint": "eslint --ext .tsx,.js,.ts,.jsx src",
    "podlet": "node podlet.js"
  },
  "eslintConfig": {
    "plugins": [
      "jsx-a11y"
    ],
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:jsx-a11y/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "printWidth": 120
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test-ci"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,md,ts,tsx}": "prettier --write"
  }
}
